{
  "expo": {
    "name": "EatforGood",
    "description": "EatforGood permet de facilement se renseigner sur l'empreinte carbone de son alimentation",
    "slug": "EatforGood",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.16",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to identify food"
      },
      "supportsTablet": true,
      "bundleIdentifier": "com.EatforGood.EatforGood",
      "icon": "./assets/icon.png"
    },
    "android": {
      "package": "com.EatforGood.EatforGood",
      "permissions": ["CAMERA"],
      "icon": "./assets/icon.png",
      "versionCode": 16
    }
  }
}
